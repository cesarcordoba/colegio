.institucion-container
    position relative
    min-height 100vh
    display flex
    justify-content center
    align-items center
    .institucion-content
        position relative
        min-height 80vh
        width 80vw
        .linea-tiempo
            height 60vh
            .carousel
                height 100%
                .slick-list
                    height 100%
                    .slick-track
                        height 100%
                button.slick-prev
                    left inherit !important
                    right 89%
                    z-index 1
                    width 100px
                    height 100px
                    &:before
                        font-family inherit
                        content '‹'
                        font-size 100px
                        color #e2e2e2
                button.slick-next
                    right inherit !important
                    left 89%
                    width 100px
                    height 100px
                    &:before
                        font-family inherit
                        content '›'
                        font-size 100px
                        color #e2e2e2
                .slide
                    display flex
                    justify-content center
                    align-items center
                    .informacion-texto
                        position relative
                        min-height 80%
                        width 70%
                        display flex
                        flex-direction row
                        justify-content space-between
                        .texto
                            width 50%
                        .imagen-lineaTiempo
                            width 50%
                            display flex
                            justify-content center
                            align-items center
                            .imagen-anio
                                //width 308px
                                //height 357px
                                width 200px
                                height 67%
                                background-color #212121
        .linea-tiempo-horizontal
            margin-top 11vh
            display flex
            flex-direction column
            justify-content center
            align-items center
            .barra-tiempo
                background-color #767676
                width 70%
                height 1.5px
            .contenedor-linea-tiempo
                width 70%
                margin-top -9px
                display flex
                flex-direction row
                justify-content space-between
                align-items center
                .fecha
                    display flex
                    flex-direction column
                    justify-content center
                    align-items center
                    .circulo
                        width 15px
                        height 15px
                        -moz-border-radius 50%
                        -webkit-border-radius 50%
                        border-radius 50%
                        border .5px solid #767676
                        background-color white
                        //background #767676
